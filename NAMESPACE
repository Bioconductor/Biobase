             varMetadata, "annotation<-", "description<-", "eList<-", 
	     "eMetadata<-", "exprs<-", "geneNames<-", "notes<-", "pData<-", 
	     "phenoData<-", "sampleNames<-",
             "se.exprs<-",  "write.exprs")

export(Aggregate, addNonExisting, addPDF2Vig, 
